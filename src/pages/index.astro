---
import BaseLayout from "../layouts/BaseLayout.astro";
import Navigation from "../components/global/Navigation.astro";

import Hero from "../components/landing/Hero.astro";
import Contact from "../components/landing/Contact.astro";
import About from "../components/landing/About.astro";
import Services from "../components/landing/Services.astro";
import Testimonial from "../components/global/Testimonial.astro";
import Pricing from "../components/landing/Pricing.astro";
import Cta from "../components/global/Cta.astro";
import Footer from "../components/global/Footer.astro";
import { AstroSeo } from "@astrolib/seo";
---

<!---
Use AstroSeo in all the pages you want different Seo than the index page
-->
<AstroSeo
  title="Home"
  description="Security"
  canonical="https://www.53c.co.za"
  openGraph={{
    url: "https://www.53c.co.za",
    title: "Trust",
    description: "Trust and security",
    site_name: "53c",
  }}
/>
<BaseLayout>
  <Navigation />
  <Hero />
  <About />
  <Cta />
  <Services />
  <Contact />
  <Footer />
</BaseLayout>
